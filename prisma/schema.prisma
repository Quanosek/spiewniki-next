generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Hymn {
  id           Int    @id
  hymnbook     String @db.Char(1)
  number       String @db.VarChar(4)
  title        String @db.VarChar(100)
  lyrics       String
  presentation String
  verse        String
  author       String
}
